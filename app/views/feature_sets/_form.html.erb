<%= form_for @feature_set do |f| %>
    <ul>
      <% @feature_set.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
      <% end %>
    </ul>
    <ul style="list-style-type:none;
         margin:0;
         padding:0;">
   <li>
    <%= f.label :organization_id %>
    <%= f.collection_select :organization_id, Organization.all, :id, :name %>
 
 
    <%= f.label :name %>
    <%= f.text_field :name %>
 
 
    <%= f.label :price %>
    <%= f.text_field :price %>
 
 
    <%= f.label :no_students %>
    <%= f.text_field :no_students%>

   </li>
 
   <li style="float: right;
           margin-top: -19%;
           margin-right: 62%;">
      <%= f.label :no_teachers %>
      <%= f.text_field :no_teachers%>
   
   
      <%= f.label :no_admins %>
      <%= f.text_field :no_admins%>
   
   
      <%= f.label :no_courses %>
      <%= f.text_field :no_courses%>
   
   
      <%= f.label :storage %>
      <%= f.text_field :storage%>

   </li>
   
 <%= f.submit "Submit" ,:class => "btn btn-primary"%>
 </ul>
<% end %>
